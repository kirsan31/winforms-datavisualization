<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<UseWindowsForms>true</UseWindowsForms>
		<LangVersion>latest</LangVersion>
		<AssemblyName>WinForms.DataVisualization.Utilities</AssemblyName>
		<Nullable>enable</Nullable>
		<AssemblyVersion>$(MajorVer).0.0.0</AssemblyVersion>
		<FileVersion>$(MajorVer).$(MinorPatchVer).0</FileVersion>
		<InformationalVersion>$(MajorVer).$(MinorPatchVer)$(PRVer)</InformationalVersion>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<InternalsVisibleTo Include="WinForms.DataVisualization" />
		<InternalsVisibleTo Include="WinForms.DataVisualization.Designer.Client" />
		<InternalsVisibleTo Include="WinForms.DataVisualization.Designer.Server" />
		<InternalsVisibleTo Include="WinForms.DataVisualization.Designer.ClientServerProtocol" />
	</ItemGroup>

	<ItemGroup Label="Embedded Resources">
	  <EmbeddedResource Update="Resources\SR.resx">
	    <LogicalName>System.Windows.Forms.DataVisualization.Charting.SR.resources</LogicalName>
	  </EmbeddedResource>
	</ItemGroup>
</Project>
